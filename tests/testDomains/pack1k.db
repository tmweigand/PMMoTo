RayTrace {
    geometry = 1
    offset = 0.0
}
Color {
    method = 7       // 1 AA BGK-SBB, 2 AA MRT-SBB, 3 AA MRT-LIBB, 4 AA TRT-SBB, 5 LIBB_TESTING, 6 NON-AA Color MRT-SBB, 7 NON-AA Color MRT-LIBB
    tauA = 1.00   // relaxation time for fluid A
    tauB = 1.00   // relaxation time for fluid B
    rhoA = 1.00     // mass density for fluid A
    rhoB = 1.00     // mass density for fluid B
    alpha = 0.001    // controls interfacial tension between fluids
    beta = 0.95     // controls interface width
    F = 0, 0, 0    // external body force applied to the system
    Restart = false   // restart from checkpoint file?
    din = 1.008850576         // density at inlet (if external BC is applied)
    dout = 0.991149424                 // density at outlet (if external BC is applied )
    timestepMax = 2000000        // maximum number of timesteps to simulate
    flux = 0.0                 // volumetric flux in voxels per timestep (if flux BC is applied)
    contact_angle = 10.0
    FluidInitialization = 0
}

Domain {
    Filename = "None.raw"
    ReadType = "8bit"
    FromRaw = false
    nproc = 5, 5, 6      // Number of processors (Npx,Npy,Npz)
    n = 200, 200, 254     // Size of local domain (Nx,Ny,Nz)
    N = 1000, 1000, 1524
    nspheres = 1685        // Number of spheres (only needed if using a sphere packing)
    L = 0.1880396149818617, 0.1880396149818617, 0.2865723732323572 // Length of domain (x,y,z)
    BC = 3                 // Boundary condition type
    usr = "kbruning"
    filesystem = 0
    RandomSaturation = 0.01
    InitialSaturation = 1
    ReadValues = 0, 1, 2           // list of labels within the binary file (read)
    WriteValues = 0, 1, 2         // list of labels within the output files (write)
    legacySDs = false
    deltaR = 0.1
    ReservoirMin = 16      //First voxel outside reservoir region
    RservoirMax = 1510     //Last voxel inside reservoir region
    AnalysisMin = 127       //First voxel inside analysis region
    AnalysisMax = 1126     //Last voxel inside analysis region
}

Analysis {
    blobid_interval = 50000000      // Frequency to perform blob identification
    analysis_interval = 10000    // Frequency to perform analysis
    restart_interval = 100000   // Frequency to write restart data
    visualization_interval = 500000        // Frequency to write visualization data
    component_interval = 20000000
    restart_file = "Restart"    // Filename to use for restart file (will append rank)
    N_threads    = 7            // Number of threads to use
    load_balance = "none" // Load balance method to use: "none", "default", "independent"
    subdivide = 5, 5, 6
    CaThreshold = 1E-3
}
